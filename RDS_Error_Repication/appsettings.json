{
	"UseInMemoryDatabase": false,
	"ConnectionStrings": {
		"DefaultConnection": "Host = localhost; Database = cliqueDev; Username = postgres; Pwd = secret; "
	},
	"Logging": {
		"Console": {
			"LogLevel": {
				"Default": "Information",
				"System.Net.Http*": "Warning",
				"System.Net.Http.HttpClient*": "Warning"
			}
		},
		"Debug": {
			"LogLevel": {
				"Default": "Information",
				"System.Net.Http*": "Warning",
				"System.Net.Http.HttpClient*": "Warning"
			}
		},
		"LogLevel": {
			"Default": "Information",
			"System.Net.Http.HttpClient*": "Warning"
		}
	},

	"STOREFRONT_URI": "",
	"STOREFRONT_KEY": "",
	"SHOPIFY_URL": "",
	"SHOP_ACCESS_TOKEN": "",

	"WP_URL": "",
	"WP_CLIENT_ID": "",
	"WP_CLIENT_SECRET": "",
	"WP_SCOPE": "",

	"AllowedHosts": "*",

	"REDIS_ENDPOINT": "localhost:6379",

	"FIREBASE_PROJECT_NAME": "cliqueDev",
	"FIREBASE_KEY": "",
	"DATABASE_NAME": "",
	"ENABLE_BENCHMARKING": false,
	"XRay": {},

	"Serilog": {
		"Using": ["Serilog.Sinks.Console"],
		"Region": "eu-central-1",
		"AllowedHosts": "*",

		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"System.Net.Http": "Warning",
				"System.Net.Http.HttpClient": "Information",
				"Microsoft.EntityFrameworkCore": "Warning",
				"StackExchange.Redis": "Warning"
			}
		},
		"WriteTo": [{ "Name": "Console" }],
		"Enrich": ["FromLogContext"],
		"Properties": {
			"Application": "Andthis"
		}
	},
